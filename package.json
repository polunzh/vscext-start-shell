{
    "name": "start-git",
    "displayName": "start-git",
    "description": "start git bash on windows",
    "version": "0.0.3",
    "publisher": "polunzh",
    "icon": "start-git.png",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.startGit"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [{
            "command": "extension.startGit",
            "title": "start git bash"
        }],
        "keybindings": [{
            "command": "extension.startGit",
            "key": "ctrl+alt+g"
        }]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "eslint": "^3.6.0",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}